{
	"name": "Bukubrow",
	"version": "1.0.1",
	"manifest_version": 2,
	"minimum_chrome_version": "41.0.2272.76",
	"description": "Synchronise your browser bookmarks with Buku",
	"homepage_url": "https://github.com/samhh/bukubrow",
	"browser_action": {
		"default_icon": "assets/icon-256.png",
		"default_popup": "content/content.html"
	},
	"options_page": "options/options.html",
	"icons": {
		"128": "assets/icon-128.png",
		"256": "assets/icon-256.png"
	},
	"background": {
		"scripts": [
			"backend.js"
		]
	},
	"permissions": [
		"nativeMessaging",
		"storage"
	],
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Alt+Shift+B"
			}
		}
	},
	"applications": {
		"gecko": {
			"id": "bukubrow@samhh.com",
			"strict_min_version": "48.0"
		}
	}
}
